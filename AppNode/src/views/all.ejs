<%- include("partials/header")  %>
          
<div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Tabla General</h3>
    </div>
    <div class="panel-body">
        <div>
            <table id="Todas" class="table table-striped table-bordered" style="width:100%">
                <thead class="thead-dark">
                  <tr>
                    <th>Nombre</th>
                    <th>Departamento</th>
                    <th>Edad</th>
                    <th>Forma de Contagio</th>
                    <th>Estado</th>
                  </tr>
                </thead>
                <tbody>
                    <%resultado.forEach(function(res){%>
                        <tr>
                            <td><%=res.Nombre%></td>
                            <td><%=res.Departamento%></td>
                            <td><%=res.Edad%></td>
                            <td><%=res.Forma_de_contagio%></td>
                            <td><%=res.Estado%></td>
                        </tr>
                    <% }) %>
                </tbody>
              </table>
        </div>
    </div>
</div>


        
           
    
        
<%- include("partials/footer")  %>

        
<%- include("partials/script")  %>