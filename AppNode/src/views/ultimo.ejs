<%- include("partials/header")  %>

<div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Ultimo Caso Agregado</h3>
    </div>
    <div class="panel-body">
        <div>
            <table class="table table-striped table-bordered" style="width:100%">
                <thead class="thead-dark">
                  <tr>
                    <th>Departamento</th>
                    <th>Nombre</th>
                    <th>Edad</th>
                    <th>Forma de Contagio</th>
                    <th>Estado</th>
                  </tr>
                </thead>
                <tbody>
                    <tr>
                        <td id="depto"></td>
                        <td id="nombre"></td>
                        <td id="edad"></td>
                        <td id="forma"></td>
                        <td id="estado"></td>
                    </tr>
                    
                </tbody>
              </table>
        </div>
    </div>
</div>
<%- include("partials/footer")  %>


<script type="text/javascript">

    // Create a variable that contains a string
    
    var myString =("<%= resultado %>").replace(/&#34;/g,"\"");
    var json = JSON.parse(myString);
    document.getElementById("nombre").innerHTML = json.Nombre;
    document.getElementById("depto").innerHTML = json.Departamento;
    document.getElementById("forma").innerHTML = json.Forma_de_contagio;
    document.getElementById("edad").innerHTML = json.Edad;
    document.getElementById("estado").innerHTML = json.Estado;
    
    
    
    
    </script>
<%- include("partials/script")  %>